{
    "name": "mrohnstock/jcarousel",
    "description": "Riding carousels with jQuery",
    "type": "component",
    "homepage": "http://sorgalla.com/jcarousel/",
    "license": "MIT",
    "keywords": [
        "carousel",
        "slider",
        "slideshow",
        "animation"
    ],
    "author": {
        "url": "http://sorgalla.com/",
        "name": "Jan Sorgalla"
    },
    "require": {
        "components/jquery": ">=1.7"
