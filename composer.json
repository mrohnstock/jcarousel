                "js/jquery.jcarousel-autoscroll.min.js",
                "js/jquery.jcarousel-control.js",
                "js/jquery.jcarousel-control.min.js",
                "js/jquery.jcarousel-core.js",
                "js/jquery.jcarousel-core.min.js",
                "js/jquery.jcarousel.js",
                "js/jquery.jcarousel.min.js",
                "js/jquery.jcarousel-pagination.js",
                "js/jquery.jcarousel-pagination.min.js",
                "js/jquery.jcarousel-scrollintoview.js",
                "js/jquery.jcarousel-scrollintoview.min.js"
            ],
            "shim": {
                "deps": ["jquery"]
            }
        }
